{
  "flow_type": "template",
  "flow_name": "header",
  "status": "released",
  "rows": [
    {
      "type": "display_group",
      "name": "dg_header",
      "condition": "@fields._deployment_name != \"plh_kids_kw\"",
      "style_list": [
        "align-items: center"
      ],
      "rows": [
        {
          "type": "image",
          "name": "icon_asset",
          "style_list": [
            "width: 46px"
          ],
          "_nested_name": "dg_header.icon_asset"
        },
        {
          "type": "title",
          "name": "title",
          "_nested_name": "dg_header.title"
        }
      ],
      "_nested_name": "dg_header",
      "_dynamicFields": {
        "condition": [
          {
            "fullExpression": "@fields._deployment_name != \"plh_kids_kw\"",
            "matchedExpression": "@fields._deployment_name",
            "type": "fields",
            "fieldName": "_deployment_name"
          }
        ]
      },
      "_dynamicDependencies": {
        "@fields._deployment_name": [
          "condition"
        ]
      }
    },
    {
      "type": "display_group",
      "name": "dg_header",
      "parameter_list": {
        "sticky": "top",
        "style": "background_solid"
      },
      "condition": "@fields._deployment_name == \"plh_kids_kw\"",
      "style_list": [
        "align-items: center",
        "padding-top: 12px",
        "padding-inline-start: 24px",
        "padding-inline-end: 12px"
      ],
      "rows": [
        {
          "name": "close_icon_asset",
          "value": "close",
          "_translations": {
            "value": {}
          },
          "exclude_from_translation": true,
          "type": "set_variable",
          "_nested_name": "dg_header.close_icon_asset"
        },
        {
          "type": "image",
          "name": "icon_asset",
          "style_list": [
            "width: 46px"
          ],
          "_nested_name": "dg_header.icon_asset"
        },
        {
          "type": "title",
          "name": "title",
          "style_list": [
            "flex: 1",
            "margin-inline-start: 12px"
          ],
          "_nested_name": "dg_header.title"
        },
        {
          "type": "round_button",
          "name": "close_button",
          "action_list": [
            {
              "trigger": "click",
              "action_id": "nav_stack",
              "args": [
                "close_top"
              ],
              "_raw": "click | nav_stack: close_top",
              "_cleaned": "click | nav_stack: close_top"
            }
          ],
          "parameter_list": {
            "variant": "no-background",
            "icon_src": "@local.close_icon_asset"
          },
          "_nested_name": "dg_header.close_button",
          "_dynamicFields": {
            "parameter_list": {
              "icon_src": [
                {
                  "fullExpression": "@local.close_icon_asset",
                  "matchedExpression": "@local.close_icon_asset",
                  "type": "local",
                  "fieldName": "close_icon_asset"
                }
              ]
            }
          },
          "_dynamicDependencies": {
            "@local.close_icon_asset": [
              "parameter_list.icon_src"
            ]
          }
        }
      ],
      "_nested_name": "dg_header",
      "_dynamicFields": {
        "condition": [
          {
            "fullExpression": "@fields._deployment_name == \"plh_kids_kw\"",
            "matchedExpression": "@fields._deployment_name",
            "type": "fields",
            "fieldName": "_deployment_name"
          }
        ]
      },
      "_dynamicDependencies": {
        "@fields._deployment_name": [
          "condition"
        ]
      }
    }
  ],
  "_xlsxPath": "templates/app_navigation.xlsx",
  "_sheetsFolderUrl": "https://drive.google.com/drive/u/0/folders/1Y8uC9-rqQtsjQgUfeX9qp-vNzsFDUQFU"
}